{% extends 'base.html.twig' %}


{% block body %}
<h3>Obecności użytkowników</h3>

<ul>
    {% for user in users %}
        <li>
            <a href="{{ path('presence_show', {'userId': user.id}) }}">{{ user.username }}</a>
        </li>
    {% endfor %}
</ul>

<h3>Obecności Ostatnich dni</h3>

<ul>
    {% for date in dates %}
        <li>
            <a href="{{ path('presence_show_date', {'date': date}) }}">{{ date }}</a>
        </li>
    {% endfor %}
</ul>

{% endblock %}